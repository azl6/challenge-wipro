resource "aws_security_group" "SG_Proxy" {
  name        = "SG_Proxy_Wipro"
  description = "Allows traffic from everywhere"

  ingress {
    from_port   = 0
    to_port     = 0
    protocol    = "all"
    cidr_blocks = ["0.0.0.0/0"]
  }

  egress {
    from_port   = 0
    to_port     = 0
    protocol    = "all"
    cidr_blocks = ["0.0.0.0/0"]
  }
}